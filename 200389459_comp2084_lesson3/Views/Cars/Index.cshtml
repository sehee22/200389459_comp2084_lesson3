
@model List<_200389459_comp2084_lesson3.Models.Car>

@{
    ViewBag.Title = "Car List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Car List</h2>

<table width="50%">
    <tr>
        <th>Id</th>
        <th>Brand</th>
        <th>Model</th>
        <th>Type</th>
        <th>Year</th>
    </tr>
    <tbody>
        @foreach (var car in Model)
        {
        <tr>
            <td>@car.Id</td>
            <td>@car.Brand</td>
            <td>@car.Model</td>
            <td>@car.Type</td>
            <td>@car.Year</td>
        </tr>
        }
    </tbody>
</table>

